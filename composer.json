{
    "name": "hnllyrp/laravel-captcha",
    "description": "The captcha package for laravel",
    "keywords": [
        "php",
        "laravel",
        "captcha"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hnllyrp",
            "email": "hnllyrp@126.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/cache": "^6.0|^9.0|^10.0",
        "illuminate/session": "^6.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^9.0|^10.0"
    },
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "Hnllyrp\\Captcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hnllyrp\\Captcha\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hnllyrp\\Captcha\\CaptchaServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
